  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev server",
      "request": "launch",
      "type": "node",
      "envFile": "${workspaceFolder}/.env.development",
      "cwd": "${workspaceFolder}/test/",
      "program": "${workspaceFolder}/test/storage-node.js",
      "runtimeArgs": [
        "--inspect",
        "--preserve-symlinks"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std"
    },
