  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev server",
      "request": "launch",
      "type": "pwa-node",
      "envFile": "${workspaceFolder}/.env.development",
      "cwd": "${workspaceFolder}/test/",
      "program": "${workspaceFolder}/test/index.js",
      "runtimeArgs": ["--inspect", "--preserve-symlinks"],
      "skipFiles": ["<node_internals>/**"],
      "outputCapture": "std"
    },
    {
      "name": "dev nodemon",
      "request": "launch",
      "type": "pwa-node",
      "envFile": "${workspaceFolder}/.env.development",
      "cwd": "${workspaceFolder}/test/",
      "program": "${workspaceFolder}/test/index.js",
      "restart": true,
      "runtimeExecutable": "nodemon",
      "skipFiles": ["<node_internals>/**"],
      "outputCapture": "std"
    },
    {
      "name": "prod server",
      "request": "launch",
      "type": "pwa-node",
      "envFile": "${workspaceFolder}/.env.production",
      "program": "${workspaceFolder}/test/index.js",
      "runtimeArgs": ["--inspect", "--preserve-symlinks"],
      "skipFiles": ["<node_internals>/**"],
      "outputCapture": "std"
    },
    {
