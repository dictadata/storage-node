{
  "name": "mysql",
  "request": {
    "method": "GET",
    "base": "http://dev.dictadata.org:8089/node/",
    "auth": "user:user"
  },
  "expected": {
    "statusCode": 200
  },
  "queries": [
    {
      "name": "status",
      "request": {
        "method": "GET",
        "url": "status/:mysql_foo_schema"
      }
    },
    {
      "name": "createSchema",
      "request": {
        "method": "PUT",
        "url": "encoding/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": "./data/input/foo_schema.encoding.json"
      },
      "expected": {
        "statusCode": [
          200,
          409
        ]
      }
    },
    {
      "name": "getEncoding",
      "request": {
        "method": "GET",
        "url": "encoding/:mysql_foo_schema"
      }
    },
    {
      "name": "list",
      "request": {
        "method": "GET",
        "url": "list/:mysql_foo_schema",
        "params": {
          "schema": "foo*"
        }
      }
    },
    {
      "name": "store fifty",
      "request": {
        "method": "PUT",
        "url": "store/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "fifty": {
            "Foo": "fifty",
            "Bar": "Grant",
            "Baz": 50,
            "Fobe": 50.5,
            "Dt Test": "2022-05-05T08:00:00",
            "enabled": true
          }
        }
      },
      "expected": {
        "compareValues": 1
      }
    },
    {
      "name": "store ten",
      "request": {
        "method": "PUT",
        "url": "store/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": [
          {
            "Foo": "ten",
            "Bar": "row",
            "Baz": 10,
            "Fobe": 10.10,
            "Dt Test": "10/27/2018",
            "enabled": true
          }
        ]
      }
    },
    {
      "name": "store primarykey",
      "request": {
        "method": "PUT",
        "url": "store/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": [
          {
            "Foo": "eleventy",
            "Bar": "row",
            "Baz": 11,
            "Fobe": 11.11,
            "Dt Test": "2011-10-07T08:00:00",
            "enabled": true
          },
          {
            "Foo": "twelvety",
            "Bar": "row",
            "Baz": 12,
            "Fobe": 12.12,
            "Dt Test": "2012-03-03T08:00:00",
            "enabled": true
          }
        ]
      }
    },
    {
      "name": "recall key",
      "request": {
        "method": "GET",
        "url": "recall/:mysql_foo_schema",
        "params": {
          "Foo": "ten"
        }
      }
    },
    {
      "name": "recall search",
      "request": {
        "method": "POST",
        "url": "recall/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Foo": "fifty"
            }
          }
        }
      }
    },
    {
      "name": "transfer",
      "request": {
        "method": "POST",
        "url": "transfer",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "tract": {
            "origin": {
              "smt": ":json_foofile"
            },
            "terminal": {
              "smt": ":mysql_foo_schema"
            }
          }
        }
      }
    },
    {
      "name": "retrieve",
      "request": {
        "method": "POST",
        "url": "retrieve/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Bar": "row",
              "Baz": {
                "gte": 100,
                "lte": 500
              }
            },
            "count": 3,
            "order": {
              "Foo": "asc"
            }
          }
        }
      }
    },
    {
      "name": "aggregate",
      "request": {
        "method": "POST",
        "url": "retrieve/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Bar": "row",
              "Baz": {
                "gte": 100,
                "lte": 500
              }
            },
            "aggregate": {
              "baz_sum": {
                "sum": "Baz"
              },
              "dt_min": {
                "min": "Dt Test"
              },
              "dt_max": {
                "max": "Dt Test"
              }
            }
          }
        }
      }
    },
    {
      "name": "groupby",
      "request": {
        "method": "POST",
        "url": "retrieve/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Bar": "row",
              "Baz": {
                "gte": 100,
                "lte": 500
              }
            },
            "aggregate": {
              "baz_sum": {
                "sum": "Baz"
              },
              "dt_min": {
                "min": "Dt Test"
              },
              "dt_max": {
                "max": "Dt Test"
              }
            }
          }
        }
      }
    },
    {
      "name": "dull key",
      "request": {
        "method": "DELETE",
        "url": "dull/:mysql_foo_schema",
        "params": {
          "Foo": "ten"
        }
      }
    },
    {
      "name": "dull search",
      "request": {
        "method": "POST",
        "url": "dull/:mysql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Foo": [
                "eleventy",
                "twelvety"
              ]
            }
          }
        }
      }
    }
  ]
}
