{
  "name": "mssql",
  "request": {
    "method": "GET",
    "base": "http://localhost:8089/node/",
    "auth": "user:user"
  },
  "expected": {
    "statusCode": 200
  },
  "queries": [
    {
      "name": "status",
      "request": {
        "method": "GET",
        "url": "status/mssql_foo_schema"
      }
    },
    {
      "name": "createSchema",
      "request": {
        "method": "PUT",
        "url": "encoding/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": "./test/data/input/foo_schema.encoding.json"
      },
      "expected": {
        "statusCode": [
          200,
          409
        ]
      }
    },
    {
      "name": "getEncoding",
      "request": {
        "method": "GET",
        "url": "encoding/mssql_foo_schema"
      }
    },
    {
      "name": "list",
      "request": {
        "method": "POST",
        "url": "list/mssql_foo_schema",
        "query": {
          "schema": "foo*"
        }
      }
    },
    {
      "name": "store twenty",
      "request": {
        "method": "PUT",
        "url": "store/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "twenty": {
            "Foo": "twenty",
            "Bar": "row",
            "Baz": 20,
            "Fobe": 20.20,
            "Dt Test": "2020-10-07T08:00:00",
            "enabled": true
          }
        }
      }
    },
    {
      "name": "store ten",
      "request": {
        "method": "PUT",
        "url": "store/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": [
          {
            "Foo": "ten",
            "Bar": "row",
            "Baz": 10,
            "Fobe": 10.10,
            "Dt Test": "10/27/2018",
            "enabled": true
          }
        ]
      }
    },
    {
      "name": "store primarykey",
      "request": {
        "method": "PUT",
        "url": "store/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": [
          {
            "Foo": "eleventy",
            "Bar": "row",
            "Baz": 11,
            "Fobe": 11.11,
            "Dt Test": "2011-10-07T08:00:00",
            "enabled": true
          },
          {
            "Foo": "twelvety",
            "Bar": "row",
            "Baz": 12,
            "Fobe": 12.12,
            "Dt Test": "2012-03-03T08:00:00",
            "enabled": true
          }
        ]
      }
    },
    {
      "name": "recall key",
      "request": {
        "method": "GET",
        "url": "recall/mssql_foo_schema",
        "query": {
          "Foo": "ten"
        }
      }
    },
    {
      "name": "recall search",
      "request": {
        "method": "POST",
        "url": "recall/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Foo": "twenty"
            }
          }
        }
      }
    },
    {
      "name": "writer",
      "request": {
        "method": "POST",
        "url": "transfer",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "tract": {
            "origin": {
              "SMT": "json_foofile"
            },
            "terminal": {
              "SMT": "mssql_foo_schema"
            }
          }
        }
      }
    },
    {
      "name": "retrieve",
      "request": {
        "method": "POST",
        "url": "retrieve/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Bar": "row",
              "Baz": {
                "gte": 100,
                "lte": 500
              }
            },
            "count": 3,
            "order": {
              "Foo": "asc"
            }
          }
        }
      }
    },
    {
      "name": "aggregate",
      "request": {
        "method": "POST",
        "url": "retrieve/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Bar": "row",
              "Baz": {
                "gte": 100,
                "lte": 500
              }
            },
            "aggregate": {
              "baz_sum": {
                "sum": "Baz"
              },
              "dt_min": {
                "min": "Dt Test"
              },
              "dt_max": {
                "max": "Dt Test"
              }
            }
          }
        }
      }
    },
    {
      "name": "groupby",
      "request": {
        "method": "POST",
        "url": "retrieve/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Bar": "row",
              "Baz": {
                "gte": 100,
                "lte": 500
              }
            },
            "aggregate": {
              "baz_sum": {
                "sum": "Baz"
              },
              "dt_min": {
                "min": "Dt Test"
              },
              "dt_max": {
                "max": "Dt Test"
              }
            }
          }
        }
      }
    },
    {
      "name": "dull key",
      "request": {
        "method": "DELETE",
        "url": "dull/mssql_foo_schema",
        "query": {
          "Foo": "ten"
        }
      }
    },
    {
      "name": "dull search",
      "request": {
        "method": "POST",
        "url": "dull/mssql_foo_schema",
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "data": {
          "pattern": {
            "match": {
              "Foo": "twelvety"
            }
          }
        }
      }
    }
  ]
}
