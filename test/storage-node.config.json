{
  "serverPort": "8089",
  "logPath": "./log/",
  "logLevel": "verbose",
  "node-api": {
    "useClientStream": true,
    "useTransport": true
  },
  "codex": {
    "smt": "elasticsearch|http://localhost:9200|dicta_codex|!name",
    "options": {
      "refresh": "true"
    },
    "engrams": {
      "accounts": "elasticsearch|http://localhost:9200|node_accounts|!userid",
      "csv_foofile": "csv|./data/input/|foofile.csv|*",
      "json_foofile": "json|./data/input/|foofile.json|*",
      "es_foo_schema": {
        "smt": "elasticsearch|http://localhost:9200|foo_schema|!Foo",
        "options": {
          "refresh": true
        }
      },
      "es_foo_schema_id": {
        "smt": "elasticsearch|http://localhost:9200|foo_schema|twenty",
        "options": {
          "refresh": true
        }
      },
      "es_foo_schema_ks": {
        "smt": "elasticsearch|http://localhost:9200|foo_schema|!",
        "options": {
          "refresh": true
        }
      },
      "es_foo_schema_pk": {
        "smt": "elasticsearch|http://localhost:9200|foo_schema_pk|=Foo",
        "options": {
          "refresh": true
        }
      },
      "es_foo_transfer": "elasticsearch|http://localhost:9200|foo_transfer|=Foo",
      "es_foo_transform": "elasticsearch|http://localhost:9200|foo_transform|=Foo",
      "mssql_foo_schema": "mssql|server=localhost;username=dicta;password=data;database=storage_node|foo_schema|=Foo",
      "mssql_foo_transfer": "mssql|server=localhost;username=dicta;password=data;database=storage_node|foo_transfer|=Foo",
      "mssql_foo_transform": "mssql|server=localhost;username=dicta;password=data;database=storage_node|foo_transform|=Foo",
      "mysql_foo_schema": "mysql|host=localhost;user=dicta;password=data;database=storage_node|foo_schema|=Foo",
      "mysql_foo_transfer": "mysql|host=localhost;user=dicta;password=data;database=storage_node|foo_transfer|=Foo",
      "mysql_foo_transform": "mysql|host=localhost;user=dicta;password=data;database=storage_node|foo_transform|=Foo",
      "oracle_foo_schema": "oracledb|connectString=localhost/xepdb1;user=dicta;password=data|foo_schema|=Foo",
      "oracle_foo_transfer": "oracledb|connectString=localhost/xepdb1;user=dicta;password=data|foo_transfer|=Foo",
      "oracle_foo_transform": "oracledb|connectString=localhost/xepdb1;user=dicta;password=data|foo_transform|=Foo",
      "rest_weather_forecast": "rest|https://api.weather.gov/gridpoints/DVN/34;71/|forecast|*",
      "es_weather_forecast": "elasticsearch|http://localhost:9200|weather_forecast|*",
      "mssql_weather_forecast": "mssql|server=localhost;username=dicta;password=data;database=storage_node|weather_forecast|*",
      "mysql_weather_forecast": "mysql|host=localhost;user=dicta;password=data;database=storage_node|weather_forecast|*",
      "oracle_weather_forecast": "oracledb|connectString=localhost/xepdb1;user=dicta;password=data|weather_forecast|*",
      "tiger2020": "elasticsearch|http://localhost:9200|us-census-tl_2020_us_state|!"
    }
  },
  "roles": {
    "Tester": "Tester"
  }
}
