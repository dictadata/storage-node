{
  "realm": "foo",
  "name": "foo_retrieve_cache",
  "type": "tract",
  "description": "retrieve data from a cacheable origin/source",
  "fibers": [
    {
      "name": "cached",
      "action": "retrieve",
      "source_fiber": "foo_source",
      "origin": {
        "smt": "foo:foo_cache",
        "pattern": {
          "match": {
            "Baz": {
              "gt": "0"
            }
          },
          "order": {
            "Foo": "asc"
          }
        }
      },
      "terminal": {
        "smt": "json|stream:*|*|*"
      }
    },
    {
      "name": "foo_source",
      "action": "tee",
      "origin": {
        "smt": ":es_foo_schema",
        "pattern": {
          "match": {
            "Baz": {
              "gt": "0"
            }
          },
          "order": {
            "Foo": "asc"
          }
        }
      },
      "terminals": [
        {
          "smt": "foo:foo_cache"
        },
        {
          "smt": "json|stream:*|*|*"
        }
      ]
    }
  ]
}
